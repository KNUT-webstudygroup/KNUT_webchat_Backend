{
  "compilerOptions": {
      "module": "nodenext",
      "target": "ES6",
      "esModuleInterop": true,
      "moduleResolution": "nodenext",
      "outDir": "dist",
      "allowJs": false,
      "skipLibCheck": true,
      "allowSyntheticDefaultImports": true,
      "strict": false,
      "forceConsistentCasingInFileNames": true,
      "resolveJsonModule": true,
      "isolatedModules": false,
      "noEmit": true,
      "jsx": "react-jsx",
      "lib": [
        "DOM",
        "DOM.Iterable",
        "ESNext"
      ],
      "typeRoots": [
        "./node_modules/@types",
        "./src/types"
      ]
      
  },
  "include": ["src/**/*","*.ts", "vite.config.mts"]

}
